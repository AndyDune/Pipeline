{
    "name": "andydune/pipeline",
    "description": "Provides a pipeline pattern. Based on middleware approach.",
    "license": "MIT",
    "keywords": [
        "php",
        "middleware",
        "pipeline"
    ],
    "homepage": "https://github.com/AndyDune/Pipeline",
    "autoload": {
        "psr-4": {
            "AndyDune\\Pipeline\\": "src/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.15 || ^6.0.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": "true"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.6-dev",
            "dev-develop": "0.6-dev"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AndyDuneTest\\": "test/"
        }
    }
}